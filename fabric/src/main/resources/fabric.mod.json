{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [ "${mod_author}" ],
    "contact": {
        "homepage": "https://github.com/Stein-N",
        "sources": "https://github.com/FabricMC/fabric-example-mod"
    },
    "license": "${mod_license}",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [ "net.xstopho.resource_backpacks.ResourceBackpacks" ],
        "client": [ "net.xstopho.resource_backpacks.ResourceBackpacksClient" ],
        "fabric-datagen": [ "net.xstopho.resource_backpacks.ResourceBackpacksDatagen" ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabric-api": "*",
        "fabricloader": "*",
        "minecraft": "${minecraft_version}",
        "java": "${java_version}",

        "resourceconfigapi": "${resource_config_api}",
        "resourcelibrary": "${resource_library}"
    }
}